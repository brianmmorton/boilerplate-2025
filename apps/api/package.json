{
  "name": "firstpick-api",
  "version": "1.0.0",
  "description": "First Pick API",
  "main": "dist/index.js",
  "repository": "https://github.com/brianmmorton/firstpick.git",
  "scripts": {
    "start": "node dist/index.js",
    "worker": "node dist/worker.js",
    "dev": "npm run dev:worker & npm run dev:api",
    "dev:worker": "cross-env NODE_ENV=development PROCESS_TYPE=worker nodemon src/worker.ts",
    "dev:api": "cross-env NODE_ENV=development PROCESS_TYPE=api nodemon src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:format": "prisma format",
    "db:migrate": "prisma migrate dev",
    "db:seed": "NODE_ENV=development ts-node src/seed/seed.ts",
    "db:update": "npm run db:format && npm run db:generate && npm run db:push",
    "build": "rimraf dist && npm run db:generate && tsc -p tsconfig.json",
    "test": "jest"
  },
  "engines": {
    "npm": "10.9.2",
    "node": "22.14.0"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r ts-node/register",
    "ext": "ts, js",
    "signal": "SIGTERM"
  },
  "author": "Brian Morton",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@flydotio/dockerfile": "^0.7.8",
    "@jest/globals": "29.3.1",
    "@types/bcrypt": "5.0.2",
    "@types/bull": "^3.15.9",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "2.8.13",
    "@types/express": "4.17.14",
    "@types/graphviz": "^0.0.32",
    "@types/jest": "29.2.5",
    "@types/lodash.groupby": "4.6.9",
    "@types/morgan": "1.9.3",
    "@types/node": "^18.11.13",
    "@types/passport": "1.0.11",
    "@types/passport-jwt": "3.0.7",
    "@types/supertest": "2.0.12",
    "@types/swagger-jsdoc": "6.0.1",
    "@types/swagger-ui-express": "4.1.3",
    "@types/xss-filters": "0.0.27",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.46.1",
    "cross-env": "7.0.3",
    "eslint": "8.29.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.3.1",
    "lint-staged": "13.3.0",
    "lodash.uniqby": "4.7.0",
    "node-mocks-http": "1.12.1",
    "nodemon": "2.0.20",
    "prettier": "2.8.1",
    "prisma": "^4.16.0",
    "prisma-json-types-generator": "^2.4.0",
    "supertest": "6.3.3",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.2"
  },
  "dependencies": {
    "@prisma/client": "4.7.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/bcryptjs": "2.4.2",
    "@types/generic-pool": "^3.1.11",
    "@types/nodemailer": "6.4.7",
    "@vercel/node": "^5.1.7",
    "ajv": "^8.17.1",
    "axios": "^1.7.9",
    "bcrypt": "5.1.1",
    "bcryptjs": "2.4.3",
    "bull": "^4.16.5",
    "bullmq": "^5.41.5",
    "compression": "1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "4.18.2",
    "express-rate-limit": "6.7.0",
    "generic-pool": "^3.9.0",
    "helmet": "6.0.1",
    "http-status": "1.5.3",
    "ioredis": "^5.5.0",
    "joi": "17.7.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lodash.groupby": "4.6.0",
    "moment": "2.29.4",
    "morgan": "1.10.0",
    "node-fetch": "^3.3.2",
    "nodemailer": "6.8.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "pm2": "5.2.2",
    "socket.io": "^4.8.1",
    "winston": "3.8.2",
    "xss-filters": "1.2.7",
    "zod": "^3.25.56"
  }
}
